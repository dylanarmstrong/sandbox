(version 1)
(deny file-write*
  (regex
    #"^/Users/[a-zA-Z0-9_]+/Library/LaunchAgents"
  )
  (subpath "/Library/LaunchAgents")
)
(deny file-read*
  (regex
    #"^/Library/Application Support/Adobe/AdobeGCClient/*"
    #"^/Applications/Utilities/*"
    #"^/Applications/Adobe Photoshop CC 2018.app/Contents/MacOS/LogTransport2.app/*"
    #"^/Applications/Adobe Photoshop CC 2018.app/Contents/Frameworks/AdobeCrashReporter.framework/Versions/A/AdobeCRDaemon.app/*"
    #"^/Applications/Adobe Photoshop CC 2018/Adobe Photoshop CC 2018.app/Contents/MacOS/LogTransport2.app/*"
    #"^/Applications/Adobe Photoshop CC 2018/Adobe Photoshop CC 2018.app/Contents/Frameworks/AdobeCrashReporter.framework/Versions/A/AdobeCRDaemon.app/*"
    #"^/Applications/Adobe Lightroom Classic CC.app/Contents/MacOS/LogTransport2.app/*"
    #"^/Applications/Adobe Lightroom Classic CC.app/Contents/Frameworks/AdobeCrashReporter.framework/Versions/A/AdobeCRDaemon.app/*"
    #"^/Applications/Adobe Lightroom Classic CC/Adobe Lightroom Classic CC.app/Contents/MacOS/LogTransport2.app/*"
    #"^/Applications/Adobe Lightroom Classic CC/Adobe Lightroom Classic CC.app/Contents/Frameworks/AdobeCrashReporter.framework/Versions/A/AdobeCRDaemon.app/*"
  )
)
(allow default)
